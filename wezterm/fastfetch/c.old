{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/master/doc/json_schema.json",

  "logo": {
    "type": "file",
    "source": "~/.config/fastfetch/logo.txt" 
  },

  "modules": [
    { "type": "os" },
    { "type": "kernel" },
    { "type": "uptime" },
    { "type": "shell" },
    { "type": "wm" },
    { "type": "cpu", "format": "{name}" },
    { "type": "gpu", "format": "{name}" },
    { "type": "memory", "format": "{used} / {total}" }
  ]
}
